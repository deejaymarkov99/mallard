{
  "extends": "airbnb",
  "rules": {
    "camelcase" : 0,
    "react/prefer-stateless-function" : 0,
    "react/jsx-filename-extension" : 0,
    "no-unused-vars": [
      "error",
      {
        "vars": "all",
        "args": "after-used",
        "ignoreRestSiblings": false,
        "argsIgnorePattern": "^_"
      }
    ]
  }
}
